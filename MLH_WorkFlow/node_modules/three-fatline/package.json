  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/three-fatline.git"
  },
  "homepage": "https://github.com/vasturiano/three-fatline",
  "keywords": [
    "3d",
    "line",
    "width",
    "webgl",
    "three"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
